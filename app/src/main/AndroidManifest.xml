<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.benahapp">


    <uses-feature android:name="android.hardware.camera" android:required="false" />
    <uses-feature android:name="android.hardware.camera.front" android:required="false" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-feature android:name="android.hardware.camera.autofocus" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Major.splashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>`
        </activity>

        <activity android:name=".Major.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.Menu" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>`
        </activity>


        <activity android:name=".attend.AttendenceActivity">
            <intent-filter>
                <action android:name="android.intent.action.ATTENDENCE" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>`
        </activity>

    <activity android:name=".attend.pluserActivity">
        <intent-filter>
            <action android:name="android.intent.action.pluserism" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

        <activity android:name=".protal.portalMain">
            <intent-filter>
                <action android:name="android.intent.action.portal" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".gallery.galleryMain">
            <intent-filter>
                <action android:name="android.intent.action.gallery" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".gallery.Detail">
            <intent-filter>
                <action android:name="android.intent.action.gallerydetails" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".course.courseMain">
            <intent-filter>
                <action android:name="android.intent.action.courses" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".calender.calenderMain">
            <intent-filter>
                <action android:name="android.intent.action.calender" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".result.resultMain">
            <intent-filter>
                <action android:name="android.intent.action.resultr" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".maps.mapMain">
            <intent-filter>
                <action android:name="android.intent.action.mapsr" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>